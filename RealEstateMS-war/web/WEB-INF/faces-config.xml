<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <description>Forward to login page</description>
        <from-view-id>*</from-view-id>
        
        <navigation-case>
            
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        
         <navigation-case>
            
            <from-outcome>registration</from-outcome>
            <to-view-id>/registration.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>advertisementApproval</from-outcome>
            <to-view-id>/admin/advertisementApproval.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subscribeAdvertisementPackage</from-outcome>
            <to-view-id>/seller/subscribeAdPackage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewRealestate</from-outcome>
            <to-view-id>/viewRealestate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updateProfile</from-outcome>
            <to-view-id>/updateProfile.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editAdPackage</from-outcome>
            <to-view-id>/admin/editAdvertisementPackage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>updatePropertyDetails</from-outcome>
            <to-view-id>/seller/userPropertyDetailsMgmt.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <description>Forward to property categories</description>
        <from-view-id>*</from-view-id>
        
        <navigation-case>
            <from-outcome>propertyCategories</from-outcome>
            <to-view-id>/admin/propertyCategoryMgmt.xhtml</to-view-id>
        </navigation-case>
      
    </navigation-rule>
    
    <navigation-rule>
        <description>Forward to advertisement package management</description>
        <from-view-id>*</from-view-id>
        
        <navigation-case>
            <from-outcome>advertisementPackages</from-outcome>
            <to-view-id>/admin/advertisementPackageMgmt.xhtml</to-view-id>
        </navigation-case>
      
    </navigation-rule>
    
      <navigation-rule>
        <description>Forward to sell realestate</description>
        <from-view-id>*</from-view-id>
        
        <navigation-case>
            <from-outcome>sellRealestate</from-outcome>
            <to-view-id>/seller/sellRealestate.xhtml</to-view-id>
        </navigation-case>
      
    </navigation-rule>
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/admin/advertisementPackageMgmt.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>removePackage</from-outcome>
            <to-view-id>/admin/removeAdvertisementPackage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/viewSearchResults.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>propertyDetails</from-outcome>
            <to-view-id>/propertyDetails.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
